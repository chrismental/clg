function doGet(e) {  const sheet = SpreadsheetApp.getActiveSpreadsheet().getSheetByName('Bookings');  const projectTitle = e.parameter.ProjectTitle;  const college = e.parameter.College;  const department = e.parameter.Department;  const data = sheet.getDataRange().getValues();  let responseData = { exists: false };  for (let i = 1; i < data.length; i++) {    if (data[i][0] == projectTitle && data[i][1] == college && data[i][2] == department) {      responseData.exists = true;      break;    }  }    return ContentService.createTextOutput(JSON.stringify(responseData))    .setMimeType(ContentService.MimeType.JSON);}function doPost(e) {  // IMPORTANT: Change this to your email address  const emailForNotifications = "your-email@example.com";  const sheet = SpreadsheetApp.getActiveSpreadsheet().getSheetByName('Bookings');  const params = e.parameter;    const newRow = [     params.projectTitle,     params.college,    params.department,    params.name,    params.email,    params.phone,    new Date()  ];    sheet.appendRow(newRow);    // Send Email Notification  const subject = "New Project Booking: " + params.projectTitle;  const body =     "A new project has been booked!\n\n" +    "Project: " + params.projectTitle + "\n" +    "College: " + params.college + "\n" +    "Department: " + params.department + "\n" +    "Name: " + params.name + "\n" +    "Email: " + params.email + "\n" +    "Phone: " + params.phone;      MailApp.sendEmail(emailForNotifications, subject, body);    return ContentService.createTextOutput(JSON.stringify({ status: 'success' }))    .setMimeType(ContentService.MimeType.JSON);}